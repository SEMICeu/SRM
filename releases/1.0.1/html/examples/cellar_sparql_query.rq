PREFIX cssv: <http://data.europa.eu/2sa/cssv#>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>


CONSTRUCT {
   ?s rdf:type dct:Standard .
   ?s dct:title ?title .
   ?s dct:description ?description .
   ?s owl:versionInfo ?versionInfo .
   ?s dct:identifier ?identifier .
   ?s foaf:homepage ?homepage .
   ?homepage rdf:type foaf:Document .
   ?s dct:creator ?owner .
   ?owner rdf:type foaf:Agent .
   ?owner dct:title ?titleowner .

}
WHERE {
   ?s rdf:type <http://data.europa.eu/2sa/cssv#Specification> .
   ?s dct:title ?title .
   ?s dct:description ?description .
   ?s <http://purl.org/pav/version> ?versionInfo .
   ?s cssv:acronym ?identifierLang .
   BIND (STR(?identifierLang)  AS ?identifier) .
   ?s <http://www.w3.org/ns/dcat#distribution> ?cssvhomepage .
   ?cssvhomepage <http://www.w3.org/ns/dcat#accessURL> ?cssvhomepage2 .
   BIND (URI(STR(?cssvhomepage2)) as ?homepage) .
   ?s cssv:isMaintainedBy ?owner .
   ?owner <http://www.w3.org/2004/02/skos/core#altLabel> ?titleowner . 
   ?s dct:relation ?relation .
   ?relation dct:relation ?related .
   FILTER (?related = <http://data.europa.eu/dr8/OntologyDataObject> || ?related = <http://data.europa.eu/dr8/LinkedOpenDataRepresentation>)
}

